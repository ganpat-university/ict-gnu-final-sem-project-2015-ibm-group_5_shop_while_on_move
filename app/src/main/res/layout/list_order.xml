<android.support.v7.widget.CardView android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardBackgroundColor="#1C1E2D"
    android:layout_margin="10dp"
    app:cardCornerRadius="10dp"
    app:cardElevation="5dp"
    xmlns:android="http://schemas.android.com/apk/res/android">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="10dp">

            <TextView
                android:id="@+id/orderid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:textColor="#22BB41"
                android:textColorHint="#22BB41"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/pname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@id/orderid"
                android:textColor="#22BB41"
                android:textColorHint="#22BB41"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/pqty"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@id/pname"
                android:textColor="#22BB41"
                android:textColorHint="#22BB41"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/pprice"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@id/pqty"
                android:textColor="#22BB41"
                android:textColorHint="#22BB41"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/paddress"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@id/pprice"
                android:textColor="#22BB41"
                android:textColorHint="#22BB41"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/pstatus"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_below="@id/paddress"
                android:textColor="#22BB41"
                android:textColorHint="#22BB41"
                android:textSize="18sp" />

        </RelativeLayout>
    </android.support.v7.widget.CardView>
<!--</RelativeLayout>-->







